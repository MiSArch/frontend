        "pinia": "^2.0.0",
        "roboto-fontface": "*",
        "vue": "^3.2.0",
        "vue-router": "^4.0.0",
        "vuetify": "^3.0.0"
    },
    "devDependencies": {
        "@babel/types": "^7.21.4",
        "@graphql-codegen/cli": "^5.0.0",
        "@graphql-codegen/typescript": "^4.0.1",
